<%= form_for(@wanderlist) do |f| %>
  <%= @wanderlist.errors.full_messages.each {|msg| puts msg } unless @wanderlist.errors.empty? %>
  <p>
    <%= f.label      :name %>
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label      :description %>
    <%= f.text_field :description %>
  </p>
  <%= f.submit %>
<% end %>
